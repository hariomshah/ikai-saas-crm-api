  case
    when (cast(now() as date) = `a`.`ScheduleDate`) then 'Current'
    when (cast(now() as date) > `a`.`ScheduleDate`) then 'Previsous'
    else 'Up-coming'
  end
  ) AS `title`,
  `a`.`ScheduleId` AS `ScheduleId`,
  `a`.`ScheduleDate` AS `ScheduleDate`,
