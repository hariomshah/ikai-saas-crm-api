  `a`.`Status` AS `Status`,
  `sts`.`MasterDesc` AS `StatusDesc`,
  `a`.`AttendantId` AS `AttendantId`,
  concat(
  `emp`.`FirstName`,
  ' ',
  `emp`.`MiddleName`,
  ' ',
  `emp`.`LastName`
  ) AS `EmpName`,
  `emp`.`ProfilePicture` AS `ProfilePicture`,
  ifnull(`vc`.`amount`, 0) AS `vc`
from
  (
  (
    (
    (
      (
      `orders_schedule` `a`
      join `slotmaster` `slt` on(
        (
        (`slt`.`CompCode` = `a`.`CompCode`)
        and (`slt`.`Id` = `a`.`SlotId`)
        )
      )
      )
      join `othermasters` `sts` on(
      (
        (`sts`.`CompCode` = `a`.`CompCode`)
