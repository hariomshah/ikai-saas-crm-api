        and (`sts`.`ShortCode` = `a`.`Status`)
      )
      )
    )
    left join `orders_schedule_visit_info` `b` on(
      (
      (`b`.`CompCode` = `a`.`CompCode`)
      and (`b`.`ScheduleId` = `a`.`ScheduleId`)
      and (`b`.`OrderId` = `a`.`OrderId`)
      )
    )
    )
    left join `employeemaster` `emp` on(
    (
      (`emp`.`CompCode` = `a`.`CompCode`)
      and (`emp`.`Id` = `a`.`AttendantId`)
    )
    )
  )
  left join (
    select
    `orders_schedule_add_on_costs`.`CompCode` AS `CompCode`,
    `orders_schedule_add_on_costs`.`ScheduleId` AS `ScheduleId`,
    sum(`orders_schedule_add_on_costs`.`Rate`) AS `amount`
    from
